<%- include('../particles/header.ejs') %>

<%- include('../particles/navigation.ejs') %>

<div class="catalog">
  <div class="catalog__head">
    <div class="catalog__title">
      <h2>Игры</h3>
    </div>
    <div class="line"></div>
  </div>
  <% for (let i = 0; i < games.length; i++) { %>
  <div class="container">
    <div class="product">
      <div class="image">
        <a href="<%= games[i].href %>">
          <img class="container-img" src="img/category/Games/<%= games[i].image %>" alt="ERROR"></a>
      </div>
      <div class="info">
        <h3><%= games[i].title %></h3>
      </div>
    </div>

  </div>
  <% } %>
</div>

<%- include('../particles/footer.ejs') %>